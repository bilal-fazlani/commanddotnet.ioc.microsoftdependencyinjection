<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.2</LangVersion>
    <Authors>Bilal Fazlani</Authors>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyTitle>CommandDotNet.IoC.MicrosoftDependencyInjection</AssemblyTitle>
    <Description>Model your command line application interface in a class</Description>
    <PackageTags>dotnet core; console; argument parse;ioc; dependency injection; commanddotnet</PackageTags>
    <PackageProjectUrl>https://github.com/bilal-fazlani/commanddotnet.ioc.microsoftdependencyinjection</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/bilal-fazlani/commanddotnet.ioc.microsoftdependencyinjection</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/bilal-fazlani/CommandDotNet/master/images/nuget-icon.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommandDotNet" Version="2.6.5" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.1.1" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Tests\**" />
  </ItemGroup>
</Project>